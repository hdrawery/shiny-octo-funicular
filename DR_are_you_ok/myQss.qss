QCalendarWidget QWidget#qt_calendar_navigationbar
{
    background-color: #323a45;

}
QCalendarWidget QToolButton {
    height: 40px;
    width: 80px;
    outline:0;
    font-family:Lucida Console;
    color: white;
    font-size: 18px;
    icon-size: 20px, 20px;
    background-color: #323a45;
}
QCalendarWidget QMenu {
    width: 80px;
    font-family:Lucida Console;
    color: white;
    font-size: 18px;
    background-color: #323a45;
}
QCalendarWidget QSpinBox {
    width: 80px;
    font-size: 18px;
    color: white;
    background-color: #323a45;
    selection-background-color: #5c6e78;
    selection-color:white;
}

QCalendarWidget QSpinBox::up-button { subcontrol-origin: border;  subcontrol-position: top right;  width:65px; }
QCalendarWidget QSpinBox::down-button {subcontrol-origin: border; subcontrol-position: bottom right;  width:65px;}
QCalendarWidget QSpinBox::up-arrow { width:20px;  height:20px; }
QCalendarWidget QSpinBox::down-arrow { width:20px;  height:20px; }

/* header row */
QCalendarWidget QWidget { alternate-background-color: #e9eef4; }

/* normal days */
QCalendarWidget QAbstractItemView:enabled
{
    font-size:20px;
    color: #323a45;
    background-color: #e9eef4;
    selection-background-color: rgb(64, 64, 64);
    selection-color: rgb(0, 255, 0);
}

/* days in other months */
QCalendarWidget QAbstractItemView:disabled { color: rgb(64, 64, 64); }



QTableWidget
{
    border:1px solid white;
    background-color:#ecedf2;

}

QTableWidget::item
{
    border:1px solid white;

}
QTableWidget::item:selected{

    border:1px solid #fc772c;

}
QScrollBar::handle:vertical
{
    background-color: #17b4d5;

    width: 10px;
}
QScrollBar::vertical
{
    border:0px;
    width: 10px;
    margin:0px 0px 0px 0px;

}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{


    background-color:#5c6e78;
}



QSlider#videoProgress::add-page
{
    background-color:white;
    height:5px;
}
QSlider#videoProgress::sub-page
{
    background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(26,188,167, 255), stop:1 rgba(23,180,213, 255));
    height:5px;
}
QSlider#videoProgress::groove
{
    background:transparent;
}
QSlider#videoProgress::handle
{

}
QSlider#check::add-page
{
    background-color:#dfdfdf;
    height:45px;
}
QSlider#check::sub-page
{
    background-color:#24ba9f;
    height:45px;
}
QSlider#check::groove
{
    background:transparent;
}
QSlider#check::handle
{
    background:white;
    border-image:url(":/img/img/slider.png");
    height:45px;
    width:60px;
    border:none;
}



