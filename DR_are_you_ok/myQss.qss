QComboBox
{
    border:1px solid #8f96a2;
    width:40px;
    color:#404040;
    font-family:Lucida Console;
    outline:0;
    padding-left:10px;

}
QComboBox:editable {
    font-family:Lucida Console;
    font-size:20px;


}
QComboBox::drop-down:editable {
    background:white;
}
QComboBox::drop-down {
     subcontrol-origin:paddind;
     subcontrol-position: top right;
     width: 35px;
     border-left-width: 0px;
     border-radius: 5px; /* same radius as the QComboBox */
}
QComboBox::down-arrow {
    image: url(":/img/img/down-arrow.png");
}
QComboBox QAbstractItemView {
     outline: 0px;
     color:black;
     background:white;
     font-family:Lucida Console;
     color:#404040;
     border:1px solid #8f96a2;
}
QComboBox QAbstractItemView::item
{
    height:40px;
    font-family:Lucida Console;
    border:none;
    color:#404040;
}


